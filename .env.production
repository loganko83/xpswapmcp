# XPSwap 프로덕션 환경 설정
DATABASE_URL=sqlite:./test.db
COINMARKETCAP_API_KEY=9589a649-0f9f-4652-8f71-e7b6abc2238e
SESSION_SECRET=production_secret_key_change_this_in_actual_production
NODE_ENV=production

# Xphere 네트워크 설정
XPHERE_RPC_URL=https://www.ankr.com/rpc/xphere/
XPHERE_CHAIN_ID=20250217

# 스마트 컨트랙트 배포 설정
DEPLOYER_PRIVATE_KEY=YOUR_ACTUAL_PRIVATE_KEY_HERE
GAS_PRICE=20000000000
GAS_LIMIT=8000000
REPORT_GAS=true

# 컨트랙트 주소 (배포 후 업데이트)
XPS_TOKEN_ADDRESS=
DEX_ROUTER_ADDRESS=
LIQUIDITY_POOL_ADDRESS=

# 프로덕션 서버 설정
PORT=5000
HOST=0.0.0.0
BASE_PATH=/xpswap

# 보안 강화 설정 (프로덕션)
HTTPS=false
SSL_CERT=./certs/server.crt
SSL_KEY=./certs/server.key
MACHINE_ID=prod-server-001
VALID_API_KEYS=xps_prod_key_001,xps_prod_key_002,xps_prod_key_003

# 보안 레벨 설정 (프로덕션 환경)
SECURITY_LEVEL=PRODUCTION
RATE_LIMIT_ENABLED=true
IP_REPUTATION_ENABLED=true
CRYPTO_SECURITY_LEVEL=HIGH

# 에러 처리 설정 (프로덕션)
ERROR_LEAK_PREVENTION=true
DETAILED_ERRORS=false
SECURITY_LOGGING=true

# API 키 설정 (프로덕션에서는 실제 키 사용)
VITE_INFURA_API_URL=https://mainnet.infura.io/v3/YOUR_INFURA_PROJECT_ID
VITE_ALCHEMY_API_URL=https://eth-mainnet.alchemyapi.io/v2/YOUR_ALCHEMY_API_KEY

# Li.Fi SDK 설정 (옵션)
VITE_LIFI_API_KEY=YOUR_LIFI_API_KEY
