  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:client": "vite",
    "dev:server": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:full": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "vite build && node esbuild.config.js",
    "build:analyze": "cross-env ANALYZE=true vite build",
    "build:client": "vite build",
    "build:server": "node esbuild.config.js",
    "build:production": "cross-env NODE_ENV=production vite build --mode production && cross-env NODE_ENV=production node esbuild.config.js",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start:production": "cross-env NODE_ENV=production BASE_PATH=/xpswap node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:security": "vitest run tests/security.test.ts",
    "test:api": "vitest run tests/api.test.ts",
    "test:components": "vitest run tests/components.test.tsx",
    "deploy:contracts": "node scripts/deploy-contracts.mjs",
    "deploy:xphere": "npx hardhat run scripts/deploy/deploy-xphere.ts --network xphere",
    "deploy:localhost": "npx hardhat run scripts/deploy/deploy-xphere.ts --network localhost",
    "hardhat:compile": "npx hardhat compile",
    "hardhat:test": "npx hardhat test",
    "hardhat:node": "npx hardhat node"
  },
